package highland2/p0dNumubarInANuModeCCQEAnalysis
version v2r12

manager Tianlu Yuan <tianlu.yuan@colorado.edu>
author nd280-software@mailman.t2k.org

branches cmt src doc app

use p0dNumubarInANuModeCCQEAnalysis * highland2

# Build methods to include.
document doxygen doxygen -group=documentation src/*.cpp src/*.h src/*.hxx src/*.cxx ../doc/*.dox

# Build information used by packages that use this one.
macro p0dNumubarInANuModeCCQEAnalysis_cppflags " -DP0DNUMUBARINANUMODECCQEANALYSIS_USED"
macro p0dNumubarInANuModeCCQEAnalysis_linkopts " -L$(P0DNUMUBARINANUMODECCQEANALYSISROOT)/$(p0dNumubarInANuModeCCQEAnalysis_tag) -lp0dNumubarInANuModeCCQEAnalysis "
macro p0dNumubarInANuModeCCQEAnalysis_stamps " $(P0DNUMUBARINANUMODECCQEANALYSISROOT)/$(p0dNumubarInANuModeCCQEAnalysis_tag)/p0dNumubarInANuModeCCQEAnalysis.stamp"

# The paths to find this library.
path_remove PATH "$(P0DNUMUBARINANUMODECCQEANALYSISROOT)"
path_prepend PATH "$(P0DNUMUBARINANUMODECCQEANALYSISROOT)/$(p0dNumubarInANuModeCCQEAnalysis_tag)"
path_remove LD_LIBRARY_PATH "$(P0DNUMUBARINANUMODECCQEANALYSISROOT)"
path_prepend LD_LIBRARY_PATH "$(P0DNUMUBARINANUMODECCQEANALYSISROOT)/$(p0dNumubarInANuModeCCQEAnalysis_tag)"
path_remove DYLD_LIBRARY_PATH "" \
	    Darwin	      "$(P0DNUMUBARINANUMODECCQEANALYSISROOT)"
path_prepend DYLD_LIBRARY_PATH "" \
	     Darwin	       "$(P0DNUMUBARINANUMODECCQEANALYSISROOT)/$(p0dNumubarInANuModeCCQEAnalysis_tag)"


# The library to build
library p0dNumubarInANuModeCCQEAnalysis *.cxx  ../dict/*.cxx

application RunP0DNumubarInANuModeCCQEAnalysis ../app/RunP0DNumubarInANuModeCCQEAnalysis*.cxx

# tests
document doxygen doxygen -group=documentation ../scripts/* ../doc/*.dox
