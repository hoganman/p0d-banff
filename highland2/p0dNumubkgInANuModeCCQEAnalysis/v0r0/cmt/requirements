package highland2/p0dNumuBkgInANuModeCCQEAnalysis
version v2r12

manager Tianlu Yuan <tianlu.yuan@colorado.edu>
author nd280-software@mailman.t2k.org

branches cmt src doc app

use p0dNumuBkgInANuModeCCQEAnalysis * highland2

# Build methods to include.
document doxygen doxygen -group=documentation src/*.cpp src/*.h src/*.hxx src/*.cxx ../doc/*.dox

# Build information used by packages that use this one.
macro p0dNumuBkgInANuModeCCQEAnalysis_cppflags " -DP0DNUMUBKGINANUMODECCQEANALYSIS_USED"
macro p0dNumuBkgInANuModeCCQEAnalysis_linkopts " -L$(P0DNUMUBKGINANUMODECCQEANALYSISROOT)/$(p0dNumuBkgInANuModeCCQEAnalysis_tag) -lp0dNumuBkgInANuModeCCQEAnalysis "
macro p0dNumuBkgInANuModeCCQEAnalysis_stamps " $(P0DNUMUBKGINANUMODECCQEANALYSISROOT)/$(p0dNumuBkgInANuModeCCQEAnalysis_tag)/p0dNumuBkgInANuModeCCQEAnalysis.stamp"

# The paths to find this library.
path_remove PATH "$(P0DNUMUBKGINANUMODECCQEANALYSISROOT)"
path_prepend PATH "$(P0DNUMUBKGINANUMODECCQEANALYSISROOT)/$(p0dNumuBkgInANuModeCCQEAnalysis_tag)"
path_remove LD_LIBRARY_PATH "$(P0DNUMUBKGINANUMODECCQEANALYSISROOT)"
path_prepend LD_LIBRARY_PATH "$(P0DNUMUBKGINANUMODECCQEANALYSISROOT)/$(p0dNumuBkgInANuModeCCQEAnalysis_tag)"
path_remove DYLD_LIBRARY_PATH "" \
	    Darwin	      "$(P0DNUMUBKGINANUMODECCQEANALYSISROOT)"
path_prepend DYLD_LIBRARY_PATH "" \
	     Darwin	       "$(P0DNUMUBKGINANUMODECCQEANALYSISROOT)/$(p0dNumuBkgInANuModeCCQEAnalysis_tag)"


# The library to build
library p0dNumuBkgInANuModeCCQEAnalysis *.cxx  ../dict/*.cxx

application RunP0DNumuBkgInANuModeCCQEAnalysis ../app/RunP0DNumuBkgInANuModeCCQEAnalysis*.cxx

# tests
document doxygen doxygen -group=documentation ../scripts/* ../doc/*.dox
